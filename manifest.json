{
    "manifest_version": 3,
    "name": "Auto Refresh Tab",
    "version": "1.0",
    "description": "Refreshes the 2nd tab every 2 minutes",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  